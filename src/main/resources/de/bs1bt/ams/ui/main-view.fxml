<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="533.0" prefWidth="655.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.bs1bt.ams.ui.MainController">
    <center>
      <GridPane>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TabPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Räume">
                  <content>
                        <VBox prefHeight="522.0" prefWidth="726.0">
                           <children>
                              <ButtonBar maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0">
                                <buttons>
                                  <Button id="btnRaumAnlegen" mnemonicParsing="false" onAction="#btnRaumAnlegenAction" text="Raum anlegen" />
                                    <Button id="btnRaumBearbeiten" mnemonicParsing="false" onAction="#btnRaumBearbeitenAction" text="Raum bearbeiten" />
                                    <Button id="btnRaumLoeschen" mnemonicParsing="false" onAction="#btnRaumLoeschenAction" text="Raum löschen" />
                                </buttons>
                              </ButtonBar>
                           </children>
                        </VBox>
                  </content>
                </Tab>
                <Tab text="Gebäude">
                  <content>
                        <VBox prefHeight="522.0" prefWidth="726.0">
                           <children>
                              <GridPane>
                                 <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                 </columnConstraints>
                                 <rowConstraints>
                                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                 </rowConstraints>
                                 <children>
                                    <TableView fx:id="gebaeudeTable">
                                      <columns>
                                        <TableColumn fx:id="columnGebaeudeId" prefWidth="50.0" text="ID" />
                                        <TableColumn fx:id="columnGebaeudeBezeichnung" prefWidth="150.0" text="Bezeichnung" />
                                        <TableColumn fx:id="columnGebaeudeAdresse" prefWidth="200.0" text="Adresse" />
                                      </columns>
                                    </TableView>
                                 </children>
                              </GridPane>
                              <ButtonBar maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0">
                                <buttons>
                                  <Button mnemonicParsing="false" onAction="#btnGebaeudeAnlegenAction" text="Gebäude anlegen" />
                                    <Button mnemonicParsing="false" onAction="#btnGebaeudeBearbeitenAction" text="Gebäude bearbeiten" />
                                    <Button mnemonicParsing="false" onAction="#btnGebaeudeLoeschenAction" text="Gebäude löschen" />
                                </buttons>
                              </ButtonBar>
                           </children>
                        </VBox>
                  </content>
                </Tab>
                <Tab text="Geräte">
                  <content>
                        <VBox prefHeight="522.0" prefWidth="726.0">
                           <children>
                              <GridPane>
                                 <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                                 </columnConstraints>
                                 <rowConstraints>
                                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                                 </rowConstraints>
                                 <children>
                                    <TableView fx:id="geraetTable">
                                      <columns>
                                        <TableColumn fx:id="columnGeraetId" prefWidth="50.0" text="ID" />
                                        <TableColumn fx:id="columnGeraetBezeichnung" prefWidth="130.0" text="Bezeichnung" />
                                          <TableColumn fx:id="columnGeraetTyp" prefWidth="100.0" text="Typ" />
                                          <TableColumn fx:id="columnGeraetRaumId" prefWidth="50.0" text="Raum ID" />
                                          <TableColumn fx:id="columnGeraetInventarnummer" prefWidth="120.0" text="Inventarnummer" />
                                      </columns>
                                    </TableView>
                                 </children>
                              </GridPane>
                              <ButtonBar maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0">
                                <buttons>
                                  <Button id="btnGeraetAnlegen" mnemonicParsing="false" onAction="#btnGeraetAnlegenAction" text="Gerät anlegen" />
                                    <Button id="btnGeraetBearbeiten" mnemonicParsing="false" onAction="#btnGeraetBearbeitenAction" text="Gerät bearbeiten" />
                                    <Button id="btnGeraetLoeschen" mnemonicParsing="false" onAction="#btnGeraetLoeschenAction" text="Gerät löschen" />
                                </buttons>
                              </ButtonBar>
                           </children>
                        </VBox>
                  </content>
                </Tab>
                  <Tab text="Mitarbeiter">
                      <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="640.0" />
                      </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </GridPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Datei">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mnuSchliessen" text="Schließen" />
            </items>
          </Menu>
            <!--
          <Menu mnemonicParsing="false" text="Werkzeuge">
            <items>
              <MenuItem mnemonicParsing="false" text="IPv4 - Checker" />
                  <MenuItem mnemonicParsing="false" text="Raumflächen-Statistik" />
            </items>
          </Menu>
          -->
          <Menu mnemonicParsing="false" text="Hilfe">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mnuUeberAMS" text="Über AMS" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
